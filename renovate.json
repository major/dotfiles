{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["\\.chezmoiexternal\\.toml$"],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>\\S+)\\s+depName=(?<depName>\\S+)\\s*\\n\\s*url\\s*=\\s*\"[^\"]*?/v?(?<currentValue>[\\d.]+)[^\"]*\""
      ],
      "versioningTemplate": "semver"
    }
  ]
}
